        function getAllegroData(manufacture, model, mainCategoryId, mainCategoryName, subCategoryId, subCategoryName, subChildId, subChildName, kioskId, topcategoryid) {
            $('#divSelectTopCategory').hide();
            $('#divCarManufactures').empty();
