        function showFooterItem(itemName) {
            //infoFranchise  infoCooperate  infoBrands  infoAbout
            if (itemName == "#infoFranchise") {
                if ($(itemName).css("display") == "none") {
                    $(itemName).css("display", "block");
                    $("#infoCooperate").css("display", "none");
                    $("#infoBrands").css("display", "none");
                    $("#infoAbout").css("display", "none");
                }
