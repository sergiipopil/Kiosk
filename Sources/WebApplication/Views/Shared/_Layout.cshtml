            if (itemName == "#infoBrands") {
                if ($(itemName).css("display") == "none") {
                    $(itemName).css("display", "block");
                    $("#infoFranchise").css("display", "none");
                    $("#infoCooperate").css("display", "none");
                    $("#infoAbout").css("display", "none");
                }
                else {
                    $(itemName).css("display", "none");
                }
            }
            if (itemName == "#infoAbout") {
                if ($(itemName).css("display") == "none") {
                    $(itemName).css("display", "block");
                    $("#infoFranchise").css("display", "none");
                    $("#infoCooperate").css("display", "none");
                    $("#infoBrands").css("display", "none");
                }
                else {
                    $(itemName).css("display", "none");
                }
            }
        }
        function openLeftMenu(menu) {
            window.history.pushState(null, null, "/" + menu);
        }
        function openNavMenu() {
            if ($("#divLeftMenu").css("display") == 'none') {
                $("#divLeftMenu").css("display", 'block');
            }
            else {
                $("#divLeftMenu").css("display", 'none');
            }
        }
        function myFunction() {
            var x = document.getElementById("myLinks");
            if (x.style.display === "block") {
                x.style.display = "none";
            } else {
                x.style.display = "block";
            }
        }
    </script>
